int sqrt(int n) {

	int y;
	int x;
	
	while (y < n) {
	
		y = y + 2 * x + 1;
		
		x = x + 1; 
	}
	
	if (y = n) {
		return x;
	}
	else {
		return x - 1;
	}
	
	//return x - 1;
}

void main() {
	
	print(sqrt(25));
	print(sqrt(26));
	print(sqrt(27));
	print(sqrt(28));
	print(sqrt(29));
	print(sqrt(30));
	print(sqrt(31));
	print(sqrt(32));
	print(sqrt(33));
	print(sqrt(34));
	print(sqrt(35));
	print(sqrt(36));
}